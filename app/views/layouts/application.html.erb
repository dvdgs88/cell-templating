<!DOCTYPE html>
<html>
<head>
  <title>CellTemplating</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
  <% @pages.each do |page| %>
    <%= link_to page.name, page_path(page) %>
  <% end %>
  <%= yield %>
</div>

</body>
</html>
